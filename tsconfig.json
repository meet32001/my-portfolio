{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "allowJs": true,
    "strictNullChecks": false
  },
  "include": ["src/**/*.{ts,tsx}"],
  "exclude": ["node_modules", "**/*.d.ts"]
}
